{
  "name": "NFL Post Tools",
  "version": "1.0.3",
  "description": "Fixes all your edit issues.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish always",
    "postinstall": "install-app-deps"
  },
  "repository": "https://github.com/addisoncoston/nfl-la-adobe-repair",
  "keywords": [
    "NFL"
  ],
  "author": "addison.coston",
  "license": "",
  "devDependencies": {
    "electron": "^15.2.0",
    "electron-builder": "^22.13.1",
    "electron-devtools-installer": "^3.2.0"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "dynamo-light": "^1.3.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.3.9",
    "font-awesome-animation": "^1.1.1",
    "fs-extra": "^10.0.0",
    "jquery": "^3.6.0",
    "moment-timezone": "^0.5.33"
  },
  "config": {},
  "build": {
    "appId": "nfl-la-adobe-repair",
    "productName": "NFL Post Tools",
    "icon": "./build/icon.ico",
    "win": {
      "target": "NSIS",
      "publish": "github"
    }
  },
  "publish": {
    "provider": "github",
    "releaseType": "release"
  }
}
